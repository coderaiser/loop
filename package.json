{
  "name": "loop",
  "private": true,
  "version": "1.0.0",
  "description": "create file system image file",
  "bin": "loop.sh",
  "scripts": {
    "lint": "shellcheck *.sh",
    "docker:build": "docker build . -t coderaiser/loop:`version`",
    "docker:push": "docker push coderaiser/loop:`version`",
    "build": "redrun docker:*",
    "wisdom": "npm run build",
    "prewisdom": "rm -f package-lock.json",
    "watcher": "nodemon -w . -e sh --exec",
    "watch:lint": "npm run watcher -- 'npm run lint'"
  },
  "keywords": [],
  "author": "coderaiser",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.11.0",
    "redrun": "^5.9.14",
    "version-io": "^2.0.1"
  }
}
